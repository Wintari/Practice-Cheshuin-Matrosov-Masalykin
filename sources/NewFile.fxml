<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.NewFileController">
   <children>
      <ToolBar prefHeight="36.0" prefWidth="600.0">
        <items>
          <Button fx:id="FirstTool" mnemonicParsing="false" prefHeight="26.0" prefWidth="27.0" />
            <Button fx:id="SecondTool" mnemonicParsing="false" prefHeight="26.0" prefWidth="27.0" />
            <Button fx:id="ThirdTool" mnemonicParsing="false" prefHeight="26.0" prefWidth="27.0" />
        </items>
      </ToolBar>
      <ButtonBar layoutY="360.0" prefHeight="40.0" prefWidth="600.0" style="-fx-background-color: BC8F8F;">
        <buttons>
          <Button fx:id="CancelButton" mnemonicParsing="false" onAction="#Cancel" text="ОТМЕА" />
            <Button fx:id="OkButton" mnemonicParsing="false" onAction="#Confirm" text="ОК" />
        </buttons>
         <padding>
            <Insets right="8.0" />
         </padding>
      </ButtonBar>
      <Pane fx:id="WorkSpace" layoutY="37.0" prefHeight="325.0" prefWidth="600.0" />
   </children>
</AnchorPane>
